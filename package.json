{
  "name": "node-haste",
  "version": "2.0.0-alpha1",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/node-haste.git"
  },
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "dependencies": {
    "absolute-path": "^0.0.0",
    "debug": "^2.2.0",
    "promise": "^7.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.20",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "fbjs-scripts": "^0.5.0",
    "eslint": "^1.9.0",
    "jest-cli": "0.7.1"
  },
  "jest": {
    "rootDir": "src",
    "scriptPreprocessor": "<rootDir>/../jest/transform"
  },
  "scripts": {
    "build": "rm -rf lib/; babel --presets=es2015,stage-2 --out-dir=lib src",
    "watch": "babel --watch --presets=es2015,stage-2 --out-dir=lib src",
    "lint": "eslint .",
    "prepublish": "npm test && npm run build",
    "test": "npm run lint && NODE_ENV=test jest"
  }
}
