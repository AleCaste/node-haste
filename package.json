{
  "name": "node-haste",
  "version": "2.0.0-alpha7",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/node-haste.git"
  },
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "dependencies": {
    "absolute-path": "^0.0.0",
    "debug": "^2.2.0",
    "graceful-fs": "^4.1.2",
    "promise": "^7.1.1",
    "sane": "^1.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-types": "^6.4.1",
    "fbjs-scripts": "^0.5.0",
    "eslint": "^1.10.3",
    "jest-cli": "0.8.2"
  },
  "jest": {
    "rootDir": "src",
    "scriptPreprocessor": "<rootDir>/../jest/transform",
    "testRunner": "<rootDir>/../node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "promise"
    ]
  },
  "scripts": {
    "build": "rm -rf lib/; babel --presets=es2015,stage-2 --out-dir=lib src",
    "watch": "babel --watch --presets=es2015,stage-2 --out-dir=lib src",
    "lint": "eslint .",
    "prepublish": "npm test && npm run build",
    "test": "npm run lint && NODE_ENV=test jest"
  }
}
